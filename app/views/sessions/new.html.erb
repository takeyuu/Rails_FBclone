<h2>Log in</h2>
<%= form_with(url: sessions_path, local: true) do |f| %>
  <div class="sign_up_form">
    <p>メールアドレス</p>
    <%= f.email_field :email %>
  </div>
  <div class="sign_up_form">
    <p>パスワード</p>
    <%= f.password_field :password, class: "is-invalid" %>
  </div>
  <div class="login_btn">
    <%= f.submit 'ログイン' %>
  </div>
<% end %>
